{
    "name": "node-tfjs-retrain",
    "version": "1.0.0",
    "description": "Transfer learning demonstration using TensorFlow.js in Node",
    "main": "app.js",
    "scripts": {
        "start": "node app.js --images_dir=\"C:/ML Demo/Projects/Colors_2/Images\" --model_dir=\"C:/ML Demo/Projects/Colors_2/tfjs-graph/\"",
        "no-retrain": "node app.js --images_dir=\"C:/ML Demo/Projects/Colors_2/Images\" --model_dir=\"C:/ML Demo/Projects/Colors_2/tfjs-graph/\" --skip_training",
        "train": "node app.js --images_dir=\"./artifacts/train-images\" --model_dir=\"./build/models\"",
        "classify": "node app.js --images_dir=\"./artifacts/validate-images\" --model_dir=\"./build/models\" --skip_training=true"
    },
    "author": "Josh Adwell",
    "license": "ISC",
    "dependencies": {
        "@tensorflow/tfjs": "^0.13.3",
        "@tensorflow/tfjs-node": "^0.1.19",
        "fast-glob": "^2.2.3",
        "fs-extra": "^7.0.0",
        "jpeg-js": "^0.3.4",
        "minimist": "^1.2.0",
        "node-fetch": "^2.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/adwellj/node-tfjs-retrain.git"
    },
    "keywords": [
        "tensorflow",
        "tensorflow.js",
        "tfjs",
        "transfer learning",
        "image classification",
        "image retraining"
    ]
}
